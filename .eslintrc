{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "json",
    "markdown"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "commonjs": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "curly": "off",
    "import/no-named-as-default": 0,
    "react/jsx-props-no-spreading": 0,
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/state-in-constructor": 0,
    "react/no-deprecated":0,
    "camelcase": 0
  }
}
